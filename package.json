{
  "name": "chootest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons",
    "deploy": "npm run build && now --static dist/ --name chooguntest && now alias"
  },
  "dependencies": {
    "choo": "7.0.0",
    "choo-devtools": "3.0.3",
    "choo-service-worker": "3.0.0",
    "gun": "0.2019.1120",
    "sheetify": "8.0.0",
    "tachyons": "4.11.1"
  },
  "devDependencies": {
    "bankai": "9.15.3",
    "choo-scaffold": "1.2.0",
    "dependency-check": "4.1.0",
    "standard": "14.3.1"
  },
  "now": {
    "name": "chooguntest",
    "alias": "chooguntest.allthethings.win"
  }
}
