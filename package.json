{
  "name": "chootest",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "bankai build index.js",
    "create": "choo-scaffold",
    "inspect": "bankai inspect index.js",
    "start": "bankai start index.js",
    "test": "standard && npm run test-deps",
    "test-deps": "dependency-check . && dependency-check . --extra --no-dev -i tachyons",
    "deploy": "npm run build && now --static dist/ --name chooguntest && now alias"
  },
  "dependencies": {
    "choo": "6.6.1",
    "choo-devtools": "2.3.3",
    "choo-service-worker": "2.3.1",
    "gun": "0.9.7",
    "sheetify": "6.2.0",
    "tachyons": "4.9.1"
  },
  "devDependencies": {
    "bankai": "9.2.0",
    "choo-scaffold": "1.1.2",
    "dependency-check": "2.10.0",
    "standard": "10.0.3"
  },
  "now": {
    "name": "chooguntest",
    "alias": "bitforbit.allthethings.win"
  }
}
